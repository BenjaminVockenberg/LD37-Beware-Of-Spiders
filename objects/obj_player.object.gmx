        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Movement Keyboard

    // Up
    if (keyboard_check(vk_up) or keyboard_check(ord('W'))) {
        // Neue Steuerung
        vspeed -= 3;
        if (vspeed &lt; -max_speed) {
            vspeed = -max_speed;
        }
    }
        
    // down    
    if (keyboard_check(vk_down) or keyboard_check(ord('S'))) {
        
        vspeed += 3;
        if (vspeed &gt; max_speed) {
            vspeed = max_speed;
        }
    }
     
    
    // Left
    if (keyboard_check(vk_left) or keyboard_check(ord('A'))) {
       
        hspeed -= 3;
        if (hspeed &lt; -max_speed) {
            hspeed = -max_speed;
        }
    }
    
    // Right
    if (keyboard_check(vk_right) or keyboard_check(ord('D'))) {
        
        hspeed += 3;
        if (hspeed &gt; max_speed) {
            hspeed = max_speed;
        }
    }
    
    
    
    
    
    
  
</string>
